<% unless notice.nil? %>
<div class="alert alert-success notice">
  <button type="button" class="close" data-dismiss="alert">&times;</button>
  <p id="notice"><%= notice %></p> 
</div>
<% end -%> 
<div class="col-lg-11 col-lg-offset-1 col-sm-10 col-sm-offset-3 col-md-8 col-md-offset-4">
<h1 class="space-title"><%=raw I18n.t ('.company_types.actions.show_company_type') %></h1>
</div>

<div class="col-lg-10">
	<div class="col-lg-12">
		<div class="col-lg-2 col-xs-5 text-right">
			<strong><%= raw I18n.t('.company_types.attributes.name') %></strong>
		</div>
		<div class="col-lg-10 col-xs-7 rdown">
			<div class="col-lg-10 ">
				 <%= @business_company_type.name %> 
			</div>
			
		</div>
	</div>
	<div class="col-lg-12">
		<div class="col-lg-2 col-xs-5 text-right">
			 <strong><%= raw I18n.t('.company_types.attributes.limit') %></strong>
		</div>
		<div class="col-lg-10 col-xs-7 rdown">
			<div class="col-lg-10 ">
				  <%= @business_company_type.limit %> 
			</div>
			
		</div>
	</div>
</div>

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 center">      
   <%= link_to I18n.t('.general.action.edit'), edit_business_company_type_path(@business_company_type),class: "hvr-skew-backward"  %> 
<%= link_to I18n.t('.general.action.cancel'), business_company_types_path,class: "hvr-skew-backward"  %>

  </div>




 
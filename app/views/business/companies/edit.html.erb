<% unless flash[:errors].nil? %>
<div class="alert alert-danger notice">
	<button type="button" class="close" data-dismiss="alert">&times;</button>   
	<p id="notice margin_left_none"><%= flash[:errors] %></p>
</div>
<%end%>
<section class="col-lg-8 col-lg-offset-2">
	<div class="col-lg-12 edit-company">
		<h1><%= raw I18n.t('.companies.actions.edit_company') %></h1>
	</div>
	<%= render 'form', business_company: @business_company %>
</section>
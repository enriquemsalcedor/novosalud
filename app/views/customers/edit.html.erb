      <button type="button" class="close" data-dismiss="alert">&times;</button>   
       <p id="notice margin_left_none"><%= flash[:errors] %></p>
    </div>
 <%end%>
<section class="col-lg-8 col-lg-offset-2">
	<div class="col-lg-12">
		<h1 class="space-title col-lg-12"><%= raw I18n.t('.customers.actions.edit_customer')%></h1>
		<%= render 'form', customer: @customer %>
	</div>
</section>


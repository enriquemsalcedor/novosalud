<%= form_for  @security_role_profile, remote: true, html: {role: :form, 'data-model' => 'security_role_profile'} do |f| %>
<div class="col-lg-12 form-group">
  <div class="col-lg-12 input_100 btn ">
  <label class="label-form">Asignar Rol</label>
    <%= collection_select(:security_role_profile, :security_role_id, @security_roles, :id, :name, prompt: true,:class => "miselect" %>
    <span class="help-block"></span>
  </div>
</div>
<div class="col-lg-12 form-group">
  <div class="col-lg-2">
    <label class="label-form">Asignar Tipo de Rol</label>
  </div>
  <div class="col-lg-12 input_100 btn">
    <%= collection_select(:security_role_profile, :security_role_type_id, @security_role_types, :id, :name, prompt: true  %>
    <span class="help-block"></span>
  </div>
</div>
<div class="col-lg-12 form-group">
  <div class="col-lg-12  col-sm-6 input_100 input-daterange calendar">
  <%= f.text_field :start_date,validate: true %>
    <span class="glyphicon glyphicon-calendar icon"></span>
    <%= f.text_field :end_date,  validate: true %>
    <span class="glyphicon glyphicon-calendar icon"></span>
  </div>
</div>
<div class="col-lg-12 form-group ">
  <div  class="col-lg-2 col-lg-offset-7 col-sm-2 col-sm-offset-7 col-md-2 col-md-offset-7">
    <%= link_to I18n.t('.general.action.cancel'), security_profiles_path,class: "hvr-skew-backward" %>
  </div>
  <div class="col-lg-3 pull-right ">
    <%= f.submit :Aceptar,:id=>'btnAceptarRoleProfile',class: "hvr-skew-backward",data: {disable_with: "Guardando ..."} %> 
  </div>
</div>
<%end%>


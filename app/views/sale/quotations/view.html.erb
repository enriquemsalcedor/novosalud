<div class="col-lg-10">
  <div class="col-lg-12">
    <h1 class="space-title">Datos de la transferencia</h1>
  </div>
  <div class="col-lg-12">
    <div class="col-lg-2 col-xs-5 text-right">
      <strong>Banco:</strong>
    </div>
    <div class="col-lg-10 col-xs-7 rdown">
      <div class="col-lg-10 ">
        <%= @view_transference.bank.name %>
      </div>
    </div>
  </div>
  <div class="col-lg-12">
    <div class="col-lg-2 col-xs-5 text-right">
      <strong>Referencia:</strong>
    </div>
    <div class="col-lg-10 col-xs-7 rdown">
      <div class="col-lg-10 ">
        <%= @view_transference.reference %>
      </div>
    </div>
  </div>
  <div class="col-lg-12">
    <div class="col-lg-10 col-xs-7">
      <div class="col-lg-10 ">
        <%= image_tag @view_transference.image.url(:medium) %>
      </div>
    </div>
  </div>
</div>
